<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite3" parallel="methods" thread-count="3">


    <test name="Test3">
        <groups>
            <run>
                <include name="arithmetics"/>
                <include name="regression"/>
                <exclude name="trigonometricks"/>
            </run>
        </groups>
        <classes>
            <class name="unittests.arithmericaltests.DivOfDoubleTest"/>
            <class name="unittests.arithmericaltests.DivOfLongTest"/>
            <class name="unittests.arithmericaltests.MultOfLongTest"/>
            <class name="unittests.arithmericaltests.MultOfDoubleTest"/>
            <class name="unittests.arithmericaltests.PowOfDoubleTest"/>
            <class name="unittests.arithmericaltests.SqrtOfDoubleTest"/>
            <class name="unittests.arithmericaltests.SubOfDoubleTest"/>
            <class name="unittests.arithmericaltests.SubOfLongTest"/>
            <class name="unittests.arithmericaltests.SumOfDoubleTest"/>
            <class name="unittests.arithmericaltests.SumOfLongTest"/>
            <class name="unittests.trigonometricktests.SinOfDoubleTest"/>
            <class name="unittests.trigonometricktests.CosOfDoubleTest"/>
            <class name="unittests.trigonometricktests.TgOfDoubleTest"/>
            <class name="unittests.trigonometricktests.CtgOfDoubleTest"/>
        </classes>
    </test>


</suite>